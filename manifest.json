{
  "manifest_version": 2,
  "name": "ChromePoet",
  "version": "0.1",
  "content_scripts": [
    {
        "matches": [
        "<all_urls>"
        ],
        "js": ["jquery-3.2.1.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "chrome-poet.html"
  },
  "web_accessible_resources": [
    "sample-poems.json"
  ]
}